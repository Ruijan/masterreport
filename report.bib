keywords = {Ankle Joint,Ankle Joint: physiopathology,Biomechanical Phenomena,Computer Simulation,Electromyography,Electromyography: statistics {\&} numerical data,Gait,Gait Disorders, Neurologic,Gait Disorders, Neurologic: etiology,Gait Disorders, Neurologic: physiopathology,Gait: physiology,Humans,Leg,Models, Anatomic,Models, Biological,Muscle Contraction,Muscle Contraction: physiology,Muscle, Skeletal,Muscle, Skeletal: physiopathology,Stroke,Stroke: complications,Stroke: physiopathology,Stroke: rehabilitation},
