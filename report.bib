keywords = {Brain,Brain: physiology,Communication Aids for Disabled,Computer Simulation,Electroencephalography,Feedback,Humans,Man-Machine Systems,Numerical Analysis, Computer-Assisted,Reaction Time,User-Computer Interface},
month = {jan},
number = {1},
pages = {91--104},
pmid = {17499364},
title = {{Defining brain-machine interface applications by matching interface performance with device requirements.}},
