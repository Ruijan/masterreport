journal = {Lancet (London, England)},
keywords = {Arm,Artificial Limbs,Brain-Computer Interfaces,Female,Hand Strength,Humans,Microelectrodes,Middle Aged,Psychomotor Performance,Quadriplegia,Quadriplegia: therapy},
