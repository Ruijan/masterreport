keywords = {BCI,EEG,Rehabilitation,Stroke,self-paced protocol,voluntary movements},
language = {English},
month = {jul},
pages = {13},
publisher = {Frontiers},
title = {{Detection of self-paced reaching movement intention from EEG signals}},
url = {http://infoscience.epfl.ch/record/178379?ln=fr http://journal.frontiersin.org/article/10.3389/fneng.2012.00013/abstract},
volume = {5},
year = {2012}
}
