journal = {Journal of shoulder and elbow surgery / American Shoulder and Elbow Surgeons ... [et al.]},
keywords = {Aged,Arthroplasty, Replacement,Arthroplasty, Replacement: methods,Biomechanics,Deltoid Muscle,Deltoid Muscle: physiopathology,Female,Humans,Male,Middle Aged,Range of Motion, Articular,Shoulder Joint,Shoulder Joint: physiopathology,Shoulder Joint: surgery},
