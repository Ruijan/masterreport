journal = {PloS one},
keywords = {Adult,Algorithms,Ankle,Ankle: physiology,Biomechanical Phenomena,Calibration,Electromyography,Gait,Hip,Hip: physiology,Humans,Knee,Knee Joint,Knee Joint: physiology,Knee: physiology,Leg,Leg: physiology,Male,Models, Biological,Movement,Muscle Strength,Muscle, Skeletal,Muscle, Skeletal: physiology,Range of Motion, Articular,Running,Tendons,Tendons: physiology,Walking},
month = {jan},
