file = {:home/ruijan/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Lindeman et al. - 1999 - Surface EMG of proximal leg muscles in neuromuscular patients and in healthy controls. Relations to force and f.pdf:pdf},
issn = {10506411},
journal = {Journal of Electromyography and Kinesiology},
keywords = {Charcot-Marie-Tooth,fatigue,force,myotonic dystrophy,surface EMG},
month = {oct},
