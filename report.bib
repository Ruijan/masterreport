issn = {1873-5711},
journal = {Journal of electromyography and kinesiology : official journal of the International Society of Electrophysiological Kinesiology},
keywords = {Adult,Algorithms,Calibration,Computer Simulation,Electromyography,Electromyography: methods,Humans,Male,Models, Biological,Muscle Contraction,Muscle Contraction: physiology,Muscle Strength,Muscle Strength: physiology,Muscle, Skeletal,Muscle, Skeletal: physiology,Range of Motion, Articular,Reproducibility of Results,Sensitivity and Specificity,Stress, Mechanical},
