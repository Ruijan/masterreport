title = {{Myoclonic disorders: a practical approach for diagnosis and treatment.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3036960{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {4},
